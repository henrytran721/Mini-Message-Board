{
  "name": "mini-message-board",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "build-css": "node-sass --include-path scss scss/index.scss public/stylesheets/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "serverstart": "concurrently --kill-others \"npm run devstart\" \"npm run watch-css\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "heroku": "^7.39.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "moment": "^2.24.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.2"
  },
  "engines": {
    "node": "12.13.0"
  }
}
